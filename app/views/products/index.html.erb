<%= link_to '导出数据', export_products_path, class: 'btn' %>
<hr>
<table class='table'>
    <thead>
        <th>标题</th>
        <th>价格</th>
    </thead>
    <tbody>
        <% @products.each do |product| %>
            <tr>
                <td><%= product.title %></td>
                <td><%= product.price %></td>
            </tr>
        <% end %>
    </tbody>
</table>
